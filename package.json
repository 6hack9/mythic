{
  "name": "abc",
  "version": "1.0.0",
  "description": "A starter theme for WordPress.",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all --parallel build:*",
    "build:scripts": "webpack --mode production --config ./resources/build/scripts.js",
    "build:styles": "webpack --mode production --config ./resources/build/styles.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/justintadlock/abc.git"
  },
  "author": "Justin Tadlock",
  "license": "GPL-2.0+",
  "bugs": {
    "url": "https://github.com/justintadlock/abc/issues"
  },
  "homepage": "https://github.com/justintadlock/abc#readme",
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.44",
    "@babel/core": "^7.0.0-beta.44",
    "@babel/plugin-transform-runtime": "^7.0.0-beta.44",
    "@babel/preset-env": "^7.0.0-beta.44",
    "babel-loader": "^8.0.0-beta.2",
    "browserslist": "^3.2.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "cssnano": "^3.10.0",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "make-dir-cli": "^1.0.0",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "postcss-cli": "^5.0.0",
    "postcss-cssnext": "^3.1.0",
    "postcss-import": "^11.1.0",
    "postcss-loader": "^2.1.3",
    "postcss-sass": "^0.3.0",
    "postcss-scss": "^1.0.5",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "sugarss": "^1.0.1",
    "webpack": "^4.5.0",
    "webpack-cli": "^2.0.14",
    "webpack-merge": "^4.1.2"
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-transform-runtime"
    ]
  },
  "browserslist": [
    "> 1%",
    "ie >= 11",
    "last 1 Android versions",
    "last 1 ChromeAndroid versions",
    "last 2 Chrome versions",
    "last 2 Firefox versions",
    "last 2 Safari versions",
    "last 2 iOS versions",
    "last 2 Edge versions",
    "last 2 Opera versions"
  ]
}
